{
    "workbench.colorTheme": "Ros√© Pine Moon",
    "workbench.startupEditor": "none",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontSize": 13,
    "[python]": {
        "editor.formatOnType": true
    },
    "git.openRepositoryInParentFolders": "always",
    "[json]": {

        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "files.associations": {
        "*.json": "json"
    },
    "workbench.sideBar.location": "right",
    "editor.fontFamily": "Hack Nerd Font, Hack Nerd Font ,Menlo",
    "window.zoomLevel": 1,
    "go.toolsManagement.autoUpdate": true,
    "terminal.integrated.fontSize": 14,
    "git.confirmSync": false,
    "redhat.telemetry.enabled": false,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "editor.rulers": [
        //80,120
        120
    ],
    "vs-kubernetes": {
        "vs-kubernetes.crd-code-completion": "disabled"
    },
    "telemetry.feedback.enabled": false,
    "docker.extension.enableComposeLanguageServer": false,
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false
    },
    "terminal.integrated.suggest.enabled": true,
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "workbench.iconTheme": "file-icons-colourless",
    "editor.minimap.enabled": false,
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "geminicodeassist.displayInlineContextHint": false,
    "chat.tools.edits.autoApprove": {
      "**/*": true,
      "**/.vscode/*.json": false,
      "**/.env": false
    },
    "chat.tools.terminal.autoApprove": {
        "make": true,
        "just": true,
        "go install": true,
        "go get": true,
        "go mod tidy": true,
        "go build": true,
        "go test": true,
        "go run": true,
        "./bin/*": true,
        "cd": true, 
        "echo": true,
        "ls": true,
        "pwd": true,
        "cat": true,
        "head": true,
        "tail": true,
        "findstr": true,
        "wc": true,
        "tr": true,
        "cut": true,
        "cmp": true,
        "which": true,
        "basename": true,
        "dirname": true,
        "realpath": true,
        "readlink": true,
        "stat": true,
        "file": true,
        "du": true,
        "df": true,
        "sleep": true,
        "git status": true,
        "git log": true,
        "git show": true,
        "git diff": true,
        "Get-ChildItem": true,
        "Get-Content": true,
        "Get-Date": true,
        "Get-Random": true,
        "Get-Location": true,
        "Write-Host": true,
        "Write-Output": true,
        "Split-Path": true,
        "Join-Path": true,
        "Start-Sleep": true,
        "Where-Object": true,
        "/^Select-[a-z0-9]/i": true,
        "/^Measure-[a-z0-9]/i": true,
        "/^Compare-[a-z0-9]/i": true,
        "/^Format-[a-z0-9]/i": true,
        "/^Sort-[a-z0-9]/i": true,
        "column": true,
        "/^column\\b.*-c\\s+[0-9]{4,}/": false,
        "date": true,
        "/^date\\b.*(-s|--set)\\b/": false,
        "find": true,
        "/^find\\b.*-(delete|exec|execdir|fprint|fprintf|fls|ok|okdir)\\b/": false,
        "grep": true,
        "/^grep\\b.*-(f|P)\\b/": false,
        "sort": true,
        "/^sort\\b.*-(o|S)\\b/": false,
        "tree": true,
        "/^tree\\b.*-o\\b/": false,
        "/\\(.+\\)/": {
            "approve": false,
            "matchCommandLine": true
        },
        "/\\{.+\\}/": {
            "approve": false,
            "matchCommandLine": true
        },
        "/`.+`/": {
            "approve": false,
            "matchCommandLine": true
        },
        "rm": false,
        "rmdir": false,
        "del": false,
        "Remove-Item": false,
        "ri": false,
        "rd": false,
        "erase": false,
        "dd": false,
        "kill": false,
        "ps": false,
        "top": false,
        "Stop-Process": false,
        "spps": false,
        "taskkill": false,
        "taskkill.exe": false,
        "curl": false,
        "wget": false,
        "Invoke-RestMethod": false,
        "Invoke-WebRequest": false,
        "irm": false,
        "iwr": false,
        "chmod": false,
        "chown": false,
        "Set-ItemProperty": false,
        "sp": false,
        "Set-Acl": false,
        "jq": false,
        "xargs": false,
        "eval": false,
        "Invoke-Expression": false,
        "iex": false
    },
    "chat.agent.maxRequests": 300,
    "geminicodeassist.agentDebugMode": true,
    "inlineChat.notebookAgent": true,
    "geminicodeassist.enableTelemetry": false,
    "geminicodeassist.inlineSuggestions.enableAuto": false,
    "geminicodeassist.updateChannel": "Default",
    "diffEditor.codeLens": true,
    "editor.cursorStyle": "block",
}